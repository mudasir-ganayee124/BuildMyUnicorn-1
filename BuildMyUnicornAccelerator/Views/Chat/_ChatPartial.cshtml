
@using System.Configuration;
@section PageStyle
{
}


<!-- ============================================================== -->
<div class="row">
    <div class="col-12">
        <div class="card m-b-0">
            <!-- .chat-row -->
            <div id="ChatDetail" class="chat-main-box chat-main-box-detail">
                <!-- .chat-left-panel -->
                <div class="chat-left-aside chat-left-aside-detail">
                    <div class="open-panel"><i class="ti-angle-right"></i></div>
                    <input type="hidden" id="SenderName" value="@ViewBag.Accelerator.CompanyName" />

                    <input type="hidden" id="ChatSenderID" value="@ViewBag.Accelerator.StartupAcceleratorID" />
               
                    <input type="hidden" id="MessageText" />
                </div>
                <!-- .chat-left-panel -->
                <!-- .chat-right-panel -->
                <div class="chat-right-aside chat-right-aside-mini-detail" style="width:100% !important">
                    <div class="chat-main-header">
                        <div class="p-3 b-b">
                            <h4 class="box-title">Chat Message</h4>
                        </div>
                    </div>
                    <div class="chat-rbox ps ps--theme_default jschatscroll" data-ps-id="14dfe0d7-0a4a-31ef-9ebc-9a18ee08253b">
                        <ul class="chat-list chat-list-detail p-3 jsChatContentContainer" style="height: 0px; ">
                        </ul>
                        <div class="ps__scrollbar-x-rail" style="left: 0px; bottom: 0px;"><div class="ps__scrollbar-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__scrollbar-y-rail" style="top: 0px; right: 0px;"><div class="ps__scrollbar-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
                    </div>
                    <div class="card-body chat-card-body chat-card-body-detail border-top">
                        <div class="row">
                            <div class="col-8">
                                <textarea placeholder="Type your message here" class="form-control border-0 jsChatMessage"></textarea>
                            </div>
                            <div class="col-4 text-end">
                                <button type="button" class="btn btn-info btn-circle btn-lg text-white jsSendChat"><i class="fas fa-paper-plane"></i> </button>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- .chat-right-panel -->
            </div>
            <!-- /.chat-row -->
        </div>
    </div>
</div>
