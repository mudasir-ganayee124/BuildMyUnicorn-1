@section PageStyle
{
    <link href="~/assets/node_modules/toast-master/css/jquery.toast.css" rel="stylesheet">
}
<div class="row page-titles">
    <div class="col-md-5 align-self-center">
        <h4 class="text-themecolor"><a href="~/Dashboard"><i class="mdi mdi-arrow-left"></i>Back to Dashboard</a></h4>
    </div>
    <div class="col-md-7 align-self-center text-right">
        <div class="d-flex justify-content-end align-items-center">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item active">Dashboard</li>
                <li class="breadcrumb-item active">Profile</li>
            </ol>

        </div>
    </div>
</div>
<!-- ============================================================== -->
<!-- End Bread crumb and right sidebar toggle -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- Start Page Content -->
<!-- ============================================================== -->
<!-- Row -->
<div class="row">
    <!-- Column -->
    <div class="col-lg-4 col-xlg-3 col-md-5">
        <div class="card">
            <div class="card-body">
                <center class="m-t-30">
                    <img src="@ConfigurationManager.AppSettings["ClientBaseUrl"]/Content/Images/@(Model.ImageID == null ? "profile-dummy.png" : Model.ImageID)" class="img-circle" width="150" />

                    <h4 class="card-title m-t-10">@Model.CompanyName </h4>
                    <h6 class="card-subtitle"></h6>

                </center>
                <h4 class="card-title m-t-10 text-center">@Model.ContactName </h4>
            </div>
            <div>
                <hr>
            </div>
            <div class="card-body">
                <small class="text-muted">Email address </small>
                <h6>@Model.ContactEmail</h6> <small class="text-muted p-t-30 db">Phone</small>
                <h6>@Model.ClientNumber</h6> @*<small class="text-muted p-t-30 db">Address</small>
                    <h6>71 Pilgrim Avenue Chevy Chase, MD 20815</h6>*@
                @*<div class="map-box">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d470029.1604841957!2d72.29955005258641!3d23.019996818380896!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x395e848aba5bd449%3A0x4fcedd11614f6516!2sAhmedabad%2C+Gujarat!5e0!3m2!1sen!2sin!4v1493204785508" width="100%" height="150" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>*@ <small class="text-muted p-t-30 db">Social Profile</small>
                <br />

            </div>
        </div>
    </div>
    <!-- Column -->
    <!-- Column -->
    <div class="col-lg-8 col-xlg-9 col-md-7">
        <div class="card">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs profile-tab" role="tablist">

                @*<li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#profile" role="tab">Profile Settings</a> </li>*@
                <li class="nav-item"> <a class="nav-link active" data-toggle="tab" href="#settings" role="tab">Profile Settings</a> </li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content">


                <div class="tab-pane active" id="settings" role="tabpanel">

                    <div class="row">
                        <div class="col-12">

                            <div class="card">
                                <form id="frmUpdateProfile" class="form-material m-t-40" method="post">
                                    <input type="hidden" id="StartupAcceleratorID" name="StartupAcceleratorID" value="@Model.StartupAcceleratorID" />




                                    <div class="card-body">


                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Startup Name</label>
                                                    <input type="text" id="CompanyName" name="CompanyName" class="form-control form-control-line" value="@Model.CompanyName">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Email</label>
                                                    <input type="text" id="ContactName" name="ContactName" class="form-control form-control-line" value="@Model.ContactName">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>First Name</label>
                                                    <input type="text" id="ContactEmail" name="ContactEmail" class="form-control form-control-line" value="@Model.ContactEmail">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Last Name</label>
                                                    <input type="text" id="MainContact" name="MainContact" class="form-control form-control-line" value="@Model.MainContact">
                                                </div>
                                            </div>


                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>Phone</label>
                                                    <input type="text" id="Phone" name="Phone" class="form-control form-control-line" value="@Model.ClientNumber">
                                                </div>
                                            </div>


                                        </div>                                    

                                        <div class="row">
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label>Website</label>
                                                    <input type="text" id="Website" name="Website" class="form-control" value="@Model.Website">
                                                </div>
                                            </div>


                                        </div>
                                        <div class="form-group text-right">
                                            <button type="submit" class="btn btn-info">Update</button>


                                        </div>

                                    </div>


                                </form>
                            </div>






                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <!-- Column -->
</div>
@section PageScript
{
    <script src="~/assets/node_modules/toast-master/js/jquery.toast.js"></script>
   
   
}

