                                            {
                                                <i class="fas fa-video jsQuestionVideo" data-videourl="@VideoUrl" style="font-size: 14px; color: #c4302b; cursor:pointer"></i>

                                            }
                                        }


                                    </label>
                                    <textarea class='form-control form-control-line ideatextarea' id="SurveyKeyFinding" name="SurveyKeyFinding">@Model.MarketResearchResults.SurveyKeyFinding</textarea>

                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="wint1">
                                        @( LanguageList.Count() > 0 ? LanguageList.Where(x => x.PropertyName == "SurveyKeyFindingConfident").FirstOrDefault().QuestionText : "" )
                                        @{
                                            VideoUrl = QuestionList.Where(x => x.PropertyName == "SurveyKeyFindingConfident").FirstOrDefault().VideoUrl;
                                            if (!string.IsNullOrEmpty(VideoUrl))
                                            {
