                                        }

                                    </label>
                                    <textarea class='form-control form-control-line ideatextarea' id="OnlineResearchKeyFinding" name="OnlineResearchKeyFinding">@Model.MarketResearchResults.OnlineResearchKeyFinding</textarea>

                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="wint1">@( LanguageList.Count() > 0 ? LanguageList.Where(x => x.PropertyName == "OnlineResearchKeyFindingConfident").FirstOrDefault().QuestionText : "" ) </label>
                                    @Html.DropDownList("OnlineResearchKeyFindingConfident", new SelectList(ViewBag.OnlineResearchKeyFinding, "ID", "Value", @Model.MarketResearchResults.OnlineResearchKeyFindingConfident), "- Please select -", new { @class = "custom-select col-12" })

                                </div>
                            </div>

                        </div>

                    </section>


                </form>
            </div>
        </div>
    </div>

</div>








