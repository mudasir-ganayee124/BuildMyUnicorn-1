                                        }

                                    </label>
                                    @Html.DropDownList("SurveyKeyFindingConfident", new SelectList(ViewBag.SurveyKeyFinding, "ID", "Value", @Model.MarketResearchResults.SurveyKeyFindingConfident), "- Please select -", new { @class = "custom-select col-12" })

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="wint1">
                                        @( LanguageList.Count() > 0 ? LanguageList.Where(x => x.PropertyName == "OnlineResearchKeyFinding").FirstOrDefault().QuestionText : "" )
                                        @{
                                            VideoUrl = QuestionList.Where(x => x.PropertyName == "OnlineResearchKeyFinding").FirstOrDefault().VideoUrl;
                                            if (!string.IsNullOrEmpty(VideoUrl))
                                            {
                                                <i class="fas fa-video jsQuestionVideo" data-videourl="@VideoUrl" style="font-size: 14px; color: #c4302b; cursor:pointer"></i>

