                                    <textarea class='form-control form-control-line ideatextarea' id="ObservationKeyFinding" name="ObservationKeyFinding">@Model.MarketResearchResults.ObservationKeyFinding</textarea>

                                </div>
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="wint1">
                                        @( LanguageList.Count() > 0 ? LanguageList.Where(x => x.PropertyName == "ObservationKeyFindingConfident").FirstOrDefault().QuestionText : "" )
                                        @{
                                            VideoUrl = QuestionList.Where(x => x.PropertyName == "ObservationKeyFindingConfident").FirstOrDefault().VideoUrl;
                                            if (!string.IsNullOrEmpty(VideoUrl))
                                            {
                                                <i class="fas fa-video jsQuestionVideo" data-videourl="@VideoUrl" style="font-size: 14px; color: #c4302b; cursor:pointer"></i>

                                            }
                                        }

                                    </label>

                                    @Html.DropDownList("ObservationKeyFindingConfident", new SelectList(ViewBag.ObservationKeyFinding, "ID", "Value", @Model.MarketResearchResults.ObservationKeyFindingConfident), "- Please select -", new { @class = "custom-select col-12" })
                                </div>
                                @*<input type="hidden" id="ObservationKeyFindingConfident" name="ObservationKeyFindingConfident" value="@Model.MarketResearchResults.ObservationKeyFindingConfident" />
                    <div id="jsObservationKeyFindingConfident"></div>*@
                            </div>

                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="wint1">
                                        @( LanguageList.Count() > 0 ? LanguageList.Where(x => x.PropertyName == "SurveyKeyFinding").FirstOrDefault().QuestionText : "" )
                                        @{
                                            VideoUrl = QuestionList.Where(x => x.PropertyName == "SurveyKeyFinding").FirstOrDefault().VideoUrl;
