    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.8/css/all.css">
    <style>
        .card-body {
            flex: 1 1 auto;
            min-height: 1px;
            padding: 6px;
        }
    </style>
</head>
<body>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">Build My Unicorn</p>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->


    <section id="wrapper" class="login-register login-sidebar ajax-wrapper" style="background-image:url(../assets/images/background/login-register.jpg);">
        <div class="login-box card">
            <div class="card-body">
                <input type="hidden" id="ClientID" name="ClientID" value="" />
                <input type="hidden" id="OrderID" name="OrderID" value="" />

                <form class="form-horizontal form-material" id="frmRegister" method="post">
                    <input type="hidden" name="AffiliateLinkID" value="@ViewBag.AffiliateLinkID" />
                    <input type="hidden" name="PlanID" value="@ViewBag.PlanID" />

                    <div class="text-center">
                        <a href="javascript:void(0)" class="db"><img src="~/Content/images/buil-my-unicorn-logo.png" alt="Build My Unicorn Logo" style="width:70px;height:70px" /><br /></a>
                    </div>
                    <table width="100%">
                        <tr>
                            <td>
                                <h3 class="box-title">Register</h3>
                                <h5 class="box-title">  @ViewBag.Plan.Symbol @( Math.Round(ViewBag.Plan.Amount)), @ViewBag.Plan.PlanName</h5>
                            </td>
                            <td style="text-align:right"> <span style="font-size:12px; color:#fb9678; ">STEP 1 OF 2 </span></td>
                        </tr>
                    </table>

                    <h6>No risk. Cancel any time.</h6>
                    <div class="form-group m-t-20">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" name="StartupName" placeholder="Startup Name" autocomplete="off" data-parsley-required="true" data-parsley-required-message="Please enter startup name" />
                        </div>
                    </div>
                    <div class="form-group m-t-20">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" name="FirstName" placeholder="Your First Name" autocomplete="off" data-parsley-required="true" data-parsley-required-message="Please enter firstname" />
                            <span id="listFieldError"></span>
                        </div>
                    </div>
                    <div class="form-group m-t-20">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" name="LastName" placeholder="Your Last  Name" autocomplete="off" data-parsley-required="true" data-parsley-required-message="Please enter lastname" />
                            <span id="listFieldError"></span>
                        </div>
                    </div>
                    <div class="form-group ">
                        <div class="col-xs-12">
                            <input class="form-control" type="text" id="Email" name="Email" placeholder="Email" autocomplete="off" data-parsley-required="true" data-parsley-required-message="Please enter email" />
                        </div>
                    </div>
                    <div class="form-group m-t-20">
                        <div class="col-xs-12">
                            <input type="phone" class="form-control" name="Phone" placeholder="Phone" parsley-regexp="^\(?(?:\+?61|0)4\)?(?:[ -]?[0-9]){2}\)?(?:[ -]?[0-9]){5}[0-9]$" data-parsley-required="true" data-parsley-required-message="Please enter phone number">
                            <span id="listFieldError"></span>
                        </div>
                    </div>


                    <div class="form-group">

                        <select class="form-control custom-select" id="CountryID" name="CountryID" data-parsley-required="true" data-parsley-required-message="Please enter country"></select>
                    </div>


                    <div class="form-group row">
                        <div class="col-md-12">
                            <div class="custom-control custom-checkbox">
                                <input type="checkbox" class="custom-control-input" id="Agreement" checked>
                                <label class="custom-control-label" for="Agreement">
                                    I Agree to  <a href="javascript:void(0)">Marketing</a>
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="alert alert-danger hide alert-rounded">
                        <span id="responseMessage"></span>
                        <button type="button" class="close" onclick="$('.alert').hide()"> <span aria-hidden="true">&times;</span> </button>
                    </div>

                    <div class="form-group text-center m-t-20">
                        <div class="col-xs-12">

                            <button class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light" type="submit">Continue  &nbsp;&nbsp; <i class="fa fa-arrow-right pull-right"></i></button>
                        </div>
                    </div>
                    <div class="form-group m-b-0">
                        <div class="col-sm-12 text-center">
                            <p>Already have an account? <a href="~/Login" class="text-info m-l-5"><b>Sign In</b></a></p>
                        </div>
                    </div>
                </form>

                <form class="form-horizontal jsfrmForgotPassword hide" id="frmPayment" method="post">
                    <div class="text-center">
                        <a href="javascript:void(0)" class="db"><img src="~/Content/images/buil-my-unicorn-logo.png" alt="Build My Unicorn Logo" style="width:70px;height:70px" /><br /></a>
                    </div>
                    <table width="100%">
                        <tr>
                            <td><h3 class="box-title">Payment</h3> </td>
                            <td style="text-align:right"> <span style="font-size:12px; color:#fb9678; ">STEP 2 OF 2 </span></td>
                        </tr>
                    </table>
                    <h5 class="box-title">  @ViewBag.Plan.Symbol @( Math.Round(ViewBag.Plan.Amount)), @ViewBag.Plan.PlanName</h5>
                    <h6>No risk. Cancel any time.</h6>
                    <div class="row">

                        <aside class="col-sm-12">


                            <article class="card">
                                <div class="card-body p-12">

                                    <ul class="nav bg-light nav-pills rounded nav-fill mb-12" role="tablist">
                                        <li class="nav-item">
                                            <a class="nav-link active" data-toggle="pill" href="#nav-tab-card">
                                                <i class="fa fa-credit-card"></i> Credit Card
                                            </a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="nav-link" data-toggle="pill" href="#nav-tab-paypal">
                                                <i class="fab fa-paypal"></i>  Paypal
                                            </a>
                                        </li>
                                        @*<li class="nav-item">
                                                <a class="nav-link" data-toggle="pill" href="#nav-tab-bank">
                                                    <i class="fa fa-university"></i>  Bank Transfer
                                                </a>
                                            </li>*@
                                    </ul>

                                    <div class="tab-content">
                                        <br />
                                        <div class="tab-pane fade show active" id="nav-tab-card">
                                            <input type="hidden" id="PublicID" value="" />
                                            <div id="card-field" style="height:20px"></div> <br />
                                            <button class="subscribe btn btn-primary btn-block" type="button" id="button-submit"> Confirm  </button>
                                            <button class="subscribe btn btn-primary btn-block hide" type="button" id="button-wait"> Please wait  </button>
                                            <div class="alert alert-danger hide alert-rounded">
                                                <span id="responsePaymentMessage"></span>
                                                <button type="button" class="close" onclick="$('.alert').hide()"> <span aria-hidden="true">&times;</span> </button>
                                            </div>

                                            @*<button id="pay-button">Pay</button>*@


                                            @*<form role="form">
                                                    <div class="form-group">
                                                        <label for="username">Full name (on the card)</label>
                                                        <input type="text" class="form-control" name="username" placeholder="" required="">
                                                    </div> <!-- form-group.// -->

                                                    <div class="form-group">
                                                        <label for="cardNumber">Card number</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" name="cardNumber" placeholder="">
                                                            <div class="input-group-append">
                                                                <span class="input-group-text text-muted">
                                                                    <i class="fab fa-cc-visa"></i>   <i class="fab fa-cc-amex"></i>
                                                                    <i class="fab fa-cc-mastercard"></i>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-sm-8">
                                                            <div class="form-group">
                                                                <label><span class="hidden-xs">Expiration</span> </label>
                                                                <div class="input-group">
                                                                    <input type="number" class="form-control" placeholder="MM" name="">
                                                                    <input type="number" class="form-control" placeholder="YY" name="">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-sm-4">
                                                            <div class="form-group">
                                                                <label data-toggle="tooltip" title="" data-original-title="3 digits code on back side of the card">CVV <i class="fa fa-question-circle"></i></label>
                                                                <input type="number" class="form-control" required="">
                                                            </div> <!-- form-group.// -->
                                                        </div>
                                                    </div>
                                                    <button class="subscribe btn btn-primary btn-block" type="button"> Confirm  </button>
                                                </form>*@
                                        </div> <!-- tab-pane.// -->
                                        <div class="tab-pane fade" id="nav-tab-paypal">
                                            <p>Paypal is easiest way to pay online</p>
                                            <p>
                                                <button type="button" class="btn btn-primary"> <i class="fab fa-paypal"></i> Log in my Paypal </button>
                                            </p>
                                            <p>
                                                <strong>Note:</strong> Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
                                                tempor incididunt ut labore et dolore magna aliqua.
                                            </p>
                                        </div>
                                        <div class="tab-pane fade" id="nav-tab-bank">
                                            <p>Bank accaunt details</p>
                                            <dl class="param">
                                                <dt>BANK: </dt>
                                                <dd> THE WORLD BANK</dd>
                                            </dl>
                                            <dl class="param">
                                                <dt>Accaunt number: </dt>
                                                <dd> 12345678912345</dd>
                                            </dl>
                                            <dl class="param">
                                                <dt>IBAN: </dt>
                                                <dd> 123456789</dd>
                                            </dl>
                                            <p>
                                                <strong>Note:</strong> Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
                                                tempor incididunt ut labore et dolore magna aliqua.
                                            </p>
                                        </div> <!-- tab-pane.// -->
                                    </div> <!-- tab-content .// -->

                                </div> <!-- card-body.// -->
                            </article> <!-- card.// -->


                        </aside>
                    </div>


                </form>
            </div>
        </div>
    </section>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- All Jquery -->
    <!-- ============================================================== -->
    <script src="~/assets/node_modules/jquery/jquery-3.2.1.min.js"></script>
    <script src="https://code.jquery.com/jquery-migrate-1.4.1.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/assets/node_modules/popper/popper.min.js"></script>
    <script src="~/assets/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/assets/node_modules/waitMe/waitMe.js"></script>
    <script src="~/Content/app_script/parsley.js"></script>
    <script src="~/page_script/global.js"></script>
    <script src="@Url.ContentVersioned("~/page_script/register.js")"></script>
    <script src="~/dist/js/pages/validation.js"></script>
</body>

</html>